<div class='col-md-9 columns'>
  <h3> Edit Address </h3>

  <%= form_for(@form_address, :url => customer_address_path(@form_address), 
  	html: {class: 'custom'}) do |f| %>
    <%= render :partial => 'form', :locals => {:f => f} %>
  <% end %>
  <div>
    <%= link_to 'Back to Addresses', customer_addresses_path, :class => 'btn' %>
  </div>
</div>
<div class="clearfix"></div>
