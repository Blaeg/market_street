<h1> New Sale </h1>

<%= form_for @sale, :url => admin_offer_sales_path(@sale) do |f| %>
  <%=  render :partial => 'form', :locals => {:f => f} %>
  <div class="actions"><%= f.submit 'Create' , :class => 'btn btn-primary' %></div>
<% end %>

<p><%= link_to "Back to List", admin_offer_sales_path, :class => 'btn btn-primary' %></p>
