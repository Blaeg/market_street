<h1> New Referral </h1>

<%= form_for @referral, :url => admin_customer_referrals_path(@referral) do |f| %>
  <%=  render :partial => 'form', :locals => {:f => f} %>
  <div class="actions"><%= f.submit 'Create' , :class => 'btn' %></div>
<% end %>

<p><%= link_to "Back to List", admin_customer_referrals_path, :class => 'btn' %></p>
