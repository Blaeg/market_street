<h3>New Deal</h3>

<%= form_for @deal, :url => admin_generic_deals_path(@deal) do |f| %>
  <%=  render :partial => 'form', :locals => {:f => f} %>
  <div class="actions"><%= f.submit 'Create' , :class => "btn btn-primary" %></div>
<% end %>

<p><%= link_to "Back to List", admin_generic_deals_path, :class => "btn btn-primary" %></p>