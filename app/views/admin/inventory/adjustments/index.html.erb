<h3>Inventory Adjustments</h3>

<table class="table table-striped table-bordered">
  <thead> 
  <tr>
    <th>Name</th>
  </tr>
  </thead>
  <% for product in @products %>
    <tr>
      <td><%= link_to product.name, admin_inventory_adjustment_path(product) %></td>
    </tr>
  <% end %>
</table>
<%= paginate @products %>