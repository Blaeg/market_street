<%= render :partial => '/admin/catalog/sub_header'%>

<h2>Edit Variant (<%= @product.name %>)</h2>
<%= form_for @variant, :url  => admin_catalog_product_variant_path(@product, @variant), :html => {:class => ''}  do |form| %>
<%= render :partial => '/admin/catalog/variants/form', 
	:locals => {:form => form}%>
<div class=" generic_submit_button">
  <%= submit_tag "Update", :class  => "btn", :id => "create_variant_button" %>
</div>
<% end %>