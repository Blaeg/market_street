<%= render :partial => '/admin/catalog/sub_header'%>
<h3>Editing Prototype <%= @prototype.name %></h3>
<%= form_for @prototype, :url  => admin_catalog_prototype_path(@prototype) do |form| %>
  <%= render :partial => '/admin/catalog/prototypes/form', 
  	:locals => {:form => form} %>
  <%= submit_tag "Update", :class  => "btn" %>
<% end %>